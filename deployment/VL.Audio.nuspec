    <tags>VL, sound, asio, wasapi, analysis, synthesis, buffer, wav, mp3, aiff, audioin, audioout, audioplayer, fft</tags>
    <dependencies>
      <group targetFramework=".NETFramework4.7.2">
		<dependency id="NAudio" version="2.0.1" />
		<dependency id="FftSharp" version="1.1.4" />
	  </group>
    </dependencies>
	<license type="expression">LGPL-3.0-only</license>
	<icon>icon\nugeticon.png</icon>
  </metadata>
  <files>
    <file src="..\lib\*.dll" target="lib\" exclude="" />
    <file src="..\lib\net472\VL.Audio.dll" target="lib\net472\" exclude="" />
    <file src="..\lib-native\**" target="lib-native\" exclude="" />
    <file src="..\Assets\**" target="Assets\" exclude="" />
    <file src="..\*.vl" target="" exclude="" />
    <file src="..\help\**" target="help\" exclude="" />
	<file src="nugeticon.png" target="icon\" />
  </files>
</package>